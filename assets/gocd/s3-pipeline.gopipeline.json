{
  "group": "defaultGroup",
  "name": "S3-pipeline",
  "display_order_weight": -1.0,
  "label_template": "${COUNT}",
  "lock_behavior": "none",
  "environment_variables": [],
  "parameters": [],
  "materials": [
    {
      "url": "https://github.com/csvpath/csvpath",
      "branch": "main",
      "shallow_clone": false,
      "filter": {
        "ignore": [],
        "includes": []
      },
      "auto_update": true,
      "type": "git"
    }
  ],
  "stages": [
    {
      "name": "test-s3",
      "fetch_materials": true,
      "never_cleanup_artifacts": false,
      "clean_working_directory": false,
      "approval": {
        "type": "success",
        "users": [],
        "roles": [],
        "allow_only_on_success": false
      },
      "environment_variables": [
      ],
      "jobs": [
        {
          "name": "unit-tests",
          "environment_variables": [
            {
              "name": "CSVPATH_CONFIG_PATH",
              "value": "\"assets/config/jenkins-s3.ini\""
            }
          ],
          "tabs": [],
          "resources": [],
          "artifacts": [],
          "timeout": 0.0,
          "tasks": [
            {
              "command": "zsh",
              "timeout": -1.0,
              "arguments": [
                "-c",
                "poetry install"
              ],
              "run_if": "passed",
              "type": "exec"
            },
            {
              "command": "zsh",
              "timeout": -1.0,
              "arguments": [
                "-c",
                "CSVPATH_CONFIG_PATH="\"assets/config/jenkins-s3.ini\"",
                "poetry run pytest"
              ],
              "run_if": "passed",
              "type": "exec"
            }
          ]
        }
      ]
    }
  ]
}
