~
   id: orders
   test-data: lookups/orders.csv
   validation-mode:print, raise
~
$[1*][
	line(
		blank.notnone(),
		string.notnone(#item),
		integer.notnone(#quantity),
		string.notnone(#sku),
		integer.notnone(#sic)
	)
	~
	not(has_dups(#0))
	~
	in(#sic, $sic-codes.headers.SIC)
	~in(#sic, $sic-codes.results.:last.lookup:data:SIC)~

 ]
